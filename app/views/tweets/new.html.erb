<h1>New Tweet</h1>
<%= form_with model: @tweet do |f| %>
    <div class='form-row'>
        <div class='form-group col-6'>
            <%= f.text_field :body, placeholder: 'Share', class: 'form-control' %>
        </div>
        <%= f.submit "Submit", class: 'btn btn-primary mt-2' %>
    </div>
    
<%end %>